      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "MONGO_CONNECTIONSTRING": "#",
        "MONGO_DATABASE_NAME": "cognizantreflectusers",
        "USER_DETAILS_COLLECTION": "userdetails",
        "ERROR_LOGS_COLLECTION": "errorlogs"
      }
